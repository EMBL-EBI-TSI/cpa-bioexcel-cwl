{
    "applicationName": "CWL VM environment",
    "contactEmail": "famaladoss@ebi.ac.uk",
    "about": "Cloud Portal Bioexcel application to create ubuntu based VM with tools such as cwltool, Toil, Docker",
    "version": "0.1",
    "cloudProviders": [
        {
            "cloudProvider": "OSTACK",
            "path": "ostack"
        }
    ],
    "inputs": [
        "nfs_server_host",
        "remote_folder"
    ],
    "deploymentParameters": [
        "disk_image_name",
        "remote_user",
        "machine_type",
        "local_folder",
        "floating_ip_pool",
        "network_name"
    ],
    "outputs": [
        "external_ip",
		"ssh_user"
    ]
}
