{
    "applicationName": "CWL VM environment",
    "contactEmail": "ecp@ebi.ac.uk",
    "about": "An EBI Cloud Portal/BioExcel application to create an Ubuntu-based VM pre-installed with tools such as cwltool, Toil, Docker",
    "version": "2.0",
    "cloudProviders": [
        {
            "cloudProvider": "OSTACK",
            "path": "ostack"
        }
    ],
    "inputs": [
        "nfs_server_host",
        "remote_folder"
    ],
    "deploymentParameters": [
        "disk_image_name",
        "remote_user",
        "machine_type",
        "local_folder",
        "floating_ip_pool",
        "network_name",
        "nfs_install",
        "workflow_url"
    ],
    "outputs": [
        "external_ip",
	"ssh_user",
	"ssh_command"
    ]
}
